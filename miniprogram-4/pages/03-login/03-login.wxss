/* pages/03-login/03-login.wxss */
.container {
  height: 100vh;
  overflow: hidden;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url(data:image/jpg;base64,);
}

.header {
  width: 200rpx;
  height: 200rpx;
  border: 2px solid red;
  border-radius: 50%;
  display: block;
  margin: 100rpx auto;
  background-color: white;
}
.content {
  /* background-color:white; */
  width: 500rpx;
  height: 400rpx;
  margin: auto;
  display: flex;
  flex-direction: column;
}
.content .formItem {
  display: flex;
  flex: 1;
  margin-bottom: 32rpx;
}

.password,
.account {
  flex: 1;
  border-bottom: 1px solid orange;
  margin-left: 10rpx;
}

.spaceBetween {
  justify-content: space-between;
}

.button {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
}